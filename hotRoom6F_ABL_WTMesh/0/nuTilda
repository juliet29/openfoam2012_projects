FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nuTilda;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 0;
    }

    outlet
    {
        type            zeroGradient;
    }

    top
    {
        type            zeroGradient;
    }

    ground
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            zeroGradient;
    }

    wtwalls
    {
        type            symmetry;
    }

    buildingFaces_master
    {
       type            zeroGradient;
    }
    buildingFaces_slave
    {
     type            zeroGradient;
    }

