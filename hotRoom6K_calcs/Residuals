"< cat PyFoamRunner.simpleFoam.logfile | grep 'Solving for Uy' | cut -d' ' -f9 | tr -d ','" title 'Uy' with lines,\
"< cat PyFoamRunner.simpleFoam.logfile | grep 'Solving for Uz' | cut -d' ' -f9 | tr -d ','" title 'Uz' with lines,\
"< cat PyFoamRunner.simpleFoam.logfile | grep 'Solving for epsilon' | cut -d' ' -f9 | tr -d ','" title 'epsilon' with lines,\
"< cat PyFoamRunner.simpleFoam.logfile | grep 'Solving for k' | cut -d' ' -f9 | tr -d ','" title 'k' with lines,\
"< cat PyFoamRunner.simpleFoam.logfile | grep 'Solving for p' | cut -d' ' -f9 | tr -d ','" title 'p' with lines
pause 300
reread