FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
                type            calculated;
        value           $internalField;
    }

    outlet
    {
        // type            fixedValue;
        // value           uniform 0;
        type            calculated;
        value           $internalField;
    }

    top
    {
        type            slip;
    }

    bottom
    {
        type            calculated;
        value           $internalField;
    }

    side01
    {
        type            symmetry;
    }

    side02
    {
        type            symmetry;
    }
    "room*" {
        type            calculated;
        value           $internalField;
    }
}

// ************************************************************************* //
